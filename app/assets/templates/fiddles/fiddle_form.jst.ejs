<% var fiddle = model.fiddle %>
<% var current_user = model.current_user %>

<section id="output">
  <iframe></iframe>
</section>

<form id="codeform">
  <textarea class="text" id="mycode" name="fiddle[method_string]"><%= fiddle.escape("method_string") %></textarea>
</form>

<div id="editor"></div>
<br>
<% var fiddle_user_id = fiddle.attributes.user_id %>
<% if ( (typeof fiddle_user_id === "undefined") || ( fiddle_user_id === current_user.id)  ) { %>
  <input type="submit" value="Save" form="codeform">
<% } %>
<button id="mybutton">Render</button>
